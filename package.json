{
  "name": "gds-stack",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "dev:yo-chat": "pnpm -C packages/framework dev --watch & pnpm -C apps/yo-chat dev",
    "build": "pnpm run -r build",
    "check": "pnpm run -r --parallel check",
    "preview": "pnpm run -r preview",
    "test": "pnpm run -r --parallel test",
    "lint": "pnpm run -r --parallel lint",
    "deploy": "pnpm build && pnpm run -r deploy"
  },
  "devDependencies": {
    "@tanstack/ts-config": "workspace:*",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@effectionx/context-api": "catalog:",
    "effection": "catalog:"
  }
}

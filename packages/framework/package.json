{
  "name": "@tanstack/framework",
  "version": "0.1.0",
  "description": "Framework utilities for TanStack-based AI apps - tool discovery, codegen, and more",
  "type": "module",
  "exports": {
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./handler": {
      "import": {
        "types": "./dist/handler.d.ts",
        "default": "./dist/handler.js"
      },
      "require": {
        "types": "./dist/handler.d.cts",
        "default": "./dist/handler.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "effection": "^4.0.0",
    "@effectionx/context-api": "^0.2.1",
    "zod": "^3.24.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "react": "catalog:",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "vitest": "catalog:",
    "effection": "catalog:",
    "@effectionx/context-api": "catalog:",
    "zod": "catalog:"
  },
  "keywords": [
    "vite",
    "tanstack",
    "framework",
    "tools",
    "codegen"
  ],
  "license": "MIT"
}

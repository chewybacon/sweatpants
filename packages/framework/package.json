{
  "name": "@tanstack/framework",
  "version": "0.1.0",
  "description": "Framework utilities for TanStack-based AI apps - tool discovery, codegen, and more",
  "type": "module",
  "exports": {
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./handler": {
      "import": {
        "types": "./dist/handler.d.ts",
        "default": "./dist/handler.js"
      },
      "require": {
        "types": "./dist/handler.d.cts",
        "default": "./dist/handler.cjs"
      }
    },
    "./chat": {
      "import": {
        "types": "./dist/chat.d.ts",
        "default": "./dist/chat.js"
      },
      "require": {
        "types": "./dist/chat.d.cts",
        "default": "./dist/chat.cjs"
      }
    },
    "./chat/isomorphic-tools": {
      "import": {
        "types": "./dist/chat/isomorphic-tools.d.ts",
        "default": "./dist/chat/isomorphic-tools.js"
      },
      "require": {
        "types": "./dist/chat/isomorphic-tools.d.cts",
        "default": "./dist/chat/isomorphic-tools.cjs"
      }
    },
    "./react/chat": {
      "import": {
        "types": "./dist/react/chat.d.ts",
        "default": "./dist/react/chat.js"
      },
      "require": {
        "types": "./dist/react/chat.d.cts",
        "default": "./dist/react/chat.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "@effectionx/context-api": "^0.2.1",
    "effection": "^4.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "zod": "^3.24.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-glob": "^3.3.2",
    "react": "^18.0.0",
    "tailwind-merge": "^2.5.2",
    "marked": "^17.0.1",
    "shiki": "^3.20.0",
    "katex": "^0.16.27",
    "mermaid": "^11.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@effectionx/context-api": "catalog:",
    "@tanstack/ts-config": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "effection": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "keywords": [
    "vite",
    "tanstack",
    "framework",
    "tools",
    "codegen"
  ],
  "license": "MIT"
}
